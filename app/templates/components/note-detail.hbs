<td class="col-sm-1">
  {{bs-input size=3 value=note.page disabled=(not isEditing)}}
</td>
<td class="col-sm-4">
  {{bs-input size=50 value=note.line disabled=(not isEditing)}}
</td>
<td class="col-sm-4">
  {{bs-input size=30 value=note.note disabled=(not isEditing)}}
</td>
<td class="col-sm-3">
  {{#bs-button-group type="radio" value=note.error}}
    {{#bs-button disabled=(and (not-eq 1 note.error) (not isEditing))
                 active=(eq 1 note.error) value=1}}1{{/bs-button}}
    {{#bs-button disabled=(and (not-eq 2 note.error) (not isEditing))
                 active=(eq 2 note.error) value=2}}2{{/bs-button}}
    {{#bs-button disabled=(and (not-eq 3 note.error) (not isEditing))
                 active=(eq 3 note.error) value=3}}3{{/bs-button}}
    {{#bs-button disabled=(and (not-eq 4 note.error) (not isEditing))
                 active=(eq 4 note.error) value=4}}4{{/bs-button}}
    {{#bs-button disabled=(and (not-eq 5 note.error) (not isEditing))
                 active=(eq 5 note.error) value=5}}5{{/bs-button}}
    {{#bs-button disabled=(and (not-eq 6 note.error) (not isEditing))
                 active=(eq 6 note.error) value=6}}6{{/bs-button}}
  {{/bs-button-group}}
</td>
<td>
  <button type="button" class="btn btn-warning"
  {{action "editNote"}}>
  {{#if isEditing}}Save{{else}}Edit{{/if}}
  </button>
</td>
<td>
  <button type="button" class="btn btn-danger" {{action (action delete note)}}>X</button>
</td>
