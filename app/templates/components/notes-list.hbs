{{#light-table table as |t|}}
  {{t.head}}

  {{#t.body as |body|}}
    {{#body.expanded-row as |row|}}
      Test {{row.page}}
    {{/body.expanded-row}}

    {{#if table.isEmpty}}
      {{#body.no-data}}
        No data.
      {{/body.no-data}}
    {{/if}}
  {{/t.body}}
{{/light-table}}
