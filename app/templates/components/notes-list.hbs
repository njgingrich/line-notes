{{#if noNotes}}
  No notes found.
{{else}}
  <thead>
    <tr>
      <th>Page</th>
      <th>Line</th>
      <th>Error</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each sortedNotes as |note|}}
      {{note-detail note=note edit=attrs.edit delete=attrs.delete}}
    {{/each}}
  </tbody>
{{/if}}
