{{#paper-sidenav class="md-whiteframe-z2" name="left"
                 lockedOpen="xl" open=leftSideBarOpen
                 onToggle=(action (mut leftSideBarOpen))}}
  {{#paper-toolbar as |toolbar|}}
    {{#paper-toolbar-tools}}
      <h2>User Profile</h2>
      <span class="flex"></span>
      {{#paper-button iconButton=true}}
        {{paper-icon "navigate-before"}}
      {{/paper-button}}
    {{/paper-toolbar-tools}}
  {{/paper-toolbar}}
  {{#paper-content padding=true}}
    {{yield}}
  {{/paper-content}}
  {{#paper-content padding=true}}
  {{/paper-content}}
{{/paper-sidenav}}
